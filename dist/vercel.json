{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user",
      "dest": "index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/post",
      "dest": "index.js"
    },
    {
        "src":"/test",
        "dest":"index.js"
    }
  ]
}

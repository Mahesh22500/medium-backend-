{
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user",
      "dest": "src/index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/post",
      "dest": "src/index.js"
    }
  ]
}
